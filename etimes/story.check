within: master
regexp: (\d+)
validator: our $ETIMES = capture()->[0]; [ $ETIMES>0, "nginx master running $ETIMES seconds" ];

validator:  \
our $ETIMES;  \
my $threshold_age = config()->{nginx}->{master_age}; \

if (config()->{nginx}->{check_master_age}){ \
  [ \
    $ETIMES <= $threshold_age , "nginx master is younger than $threshold_age seconds"  \ 
  ] \
}else{  \
  [ 1, "nginx master has right age" ]  \
}
